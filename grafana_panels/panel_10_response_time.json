{
  "id": 10,
  "type": "timeseries",
  "title": "⏱️ Среднее время ответа",
  "description": "Среднее время ответа сервера на HTTP запросы",
  "gridPos": {
    "x": 0,
    "y": 16,
    "h": 8,
    "w": 12
  },
  "fieldConfig": {
    "defaults": {
      "custom": {
        "drawStyle": "line",
        "lineInterpolation": "smooth",
        "barAlignment": 0,
        "lineWidth": 2,
        "fillOpacity": 10,
        "gradientMode": "opacity",
        "spanNulls": false,
        "insertNulls": false,
        "showPoints": "never",
        "pointSize": 5,
        "stacking": {
          "mode": "none",
          "group": "A"
        },
        "axisPlacement": "auto",
        "axisLabel": "Секунды",
        "axisColorMode": "text",
        "scaleDistribution": {
          "type": "linear"
        },
        "axisCenteredZero": false,
        "hideFrom": {
          "tooltip": false,
          "viz": false,
          "legend": false
        },
        "thresholdsStyle": {
          "mode": "line"
        }
      },
      "color": {
        "mode": "palette-classic"
      },
      "mappings": [],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {
            "color": "green",
            "value": null
          },
          {
            "color": "yellow",
            "value": 0.5
          },
          {
            "color": "red",
            "value": 1.0
          }
        ]
      },
      "unit": "s",
      "decimals": 3,
      "min": 0
    },
    "overrides": []
  },
  "options": {
    "tooltip": {
      "mode": "multi",
      "sort": "desc",
      "hideZeros": false
    },
    "legend": {
      "showLegend": true,
      "displayMode": "table",
      "placement": "bottom",
      "calcs": [
        "lastNotNull",
        "mean",
        "max"
      ],
      "width": 500
    }
  },
  "pluginVersion": "12.2.0",
  "targets": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "editorMode": "code",
      "expr": "zhevzhik_http_request_duration_seconds_avg",
      "legendFormat": "{{method}} - {{path}}",
      "range": true,
      "refId": "A"
    }
  ],
  "datasource": {
    "type": "prometheus",
    "uid": "prometheus"
  }
}


